{
    "name": "hyper/hydrator",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "php-ds/php-ds": "^1.4",
        "hyper/types": "^1.0",
        "hyper/repository": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hyper\\Hydrator\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Hyper\\Hydrator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
