{
    "name": "hyper/parser",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/log": "^1.0|^2.0|^3.0",
        "phplrt/lexer": "^3.2",
        "phplrt/parser": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Hyper\\Parser\\": "src"
        }
    },
    "require-dev": {
        "phplrt/phplrt": "^3.2",
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.8",
        "nikic/php-parser": "^4.15",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Hyper\\Parser\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
