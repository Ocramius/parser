{
    "name": "hyper/types",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "hyper/bridge": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hyper\\Type\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Hyper\\Type\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
